{
  "name": "meteor-helper",
  "main": "./lib/meteor-helper",
  "version": "0.27.0",
  "private": true,
  "description": "Launch Meteor from Atom.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/PEM--/meteor-helper"
  },
  "license": "MIT",
  "engines": {
    "atom": ">1.0.18"
  },
  "activationCommands": {
    "atom-workspace": [
      "meteor-helper:toggle",
      "meteor-helper:reset"
    ]
  },
  "dependencies": {
    "atom-space-pen-views": "^2.1.0",
    "velocity-animate": "^1.2.3"
  },
  "readmeFilename": "README.md",
  "bugs": "https://github.com/PEM--/meteor-helper/issues",
  "homepage": "https://github.com/PEM--/meteor-helper",
  "keywords": [
    "meteor",
    "launcher",
    "atom"
  ],
  "_atomModuleCache": {
    "version": 1,
    "dependencies": {
      "atom-space-pen-views": "^2.1.0",
      "velocity-animate": "^1.2.3"
    },
    "extensions": {
      ".json": [
        "package.json"
      ],
      ".coffee": [
        "lib/meteor-helper-view.coffee",
        "lib/meteor-helper.coffee"
      ],
      ".less": [
        "styles/meteor-helper.less"
      ],
      ".cson": [
        "keymaps/meteor-helper.cson",
        "menus/meteor-helper.cson"
      ],
      ".svg": [
        "assets/meteor_logo.svg"
      ]
    }
  }
}
